{{#partial "content"}}
    {{#if newTeam}}
        <p>{{newTeam.teamName}}  <a href="/restaurants/{{editPlace.id}}/update"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a></p>
        <ul>
            <li>Team Description:<br> {{newTeam.teamDescription}}</li>
        </ul>
    {{else}}
        <p>No Teams to display!</p>
    {{/if}}
{{/partial}}
{{> layout.hbs}}